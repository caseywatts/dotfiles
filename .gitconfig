[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff --color
  lg = log -p
  lp = log --oneline --graph --color
  df = diff --word-diff=color
  nudge = push --force-with-lease
[color]
  status = true
  diff = true
  ui = true
[push]
  default = simple
[credential]
	helper = osxkeychain
[user]
	email = casey.s.watts@gmail.com
[core]
	editor = atom --wait
